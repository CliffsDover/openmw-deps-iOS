(version 1)

(trace "/Users/nkasyanov/Desktop/openmwdeps.sb")

(allow default)
(deny file-read* file-write*)

(allow file-read* file-read-metadata
  (subpath "${XCODE_PATH}")
  (literal "/usr/local/bin/cmake")
  (literal "/usr/local/bin/cpack")
  (subpath "/usr/local/share/cmake")
  (subpath "/usr/local/Cellar/cmake")
